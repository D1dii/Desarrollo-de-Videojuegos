<config>
  <app>
    <title>Game Development template</title>
    <maxFrameDuration value="16"/>
  </app>

  <renderer>
    <vsync value="true"/>
  </renderer>

  <window>
    <resolution width="960" height="640" scale="2"/>
    <fullscreen value="false"/>
    <borderless value="false"/>
    <resizable value="false"/>
    <fullscreen_window value="false"/>
  </window>

  <audio>
    <music volume="128"/>
    <fx volume="128"/>
  </audio>

  <scene>
    <map path="Assets/Maps/" name="Level1 Map.tmx"/>
    <player x="100" y="1920" texturepath="Assets/Textures/idle.png"/>
  </scene>
  
</config>
